          "settings": {
            "product-image": "shopify:\/\/shop_images\/220818_NinetyPercent_SS22_03_110_6.png",
            "product-name": "<p>Organic Cotton<\/p>",
            "product-name-font": "Helvetica, Arial, sans-serif",
